// !fork returned: true
// Test basic fork functionality (just verify fork returns a value)

void main() {
    int pid = fork()
    
    // Child process should exit immediately to avoid duplicate output
    if (pid == 0) {
        exit(0)
    }
    
    // Just verify fork returned something (parent)
    if (pid >= 0) {
        println("fork returned: true")
    } else {
        println("fork returned: false")
    }
}
